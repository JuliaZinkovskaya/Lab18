Num = [1, 4, 5, 7, -9, 54, 0, 12, 25, 9, -125]

def MinElem(Num):
    min = Num[0]
    for n in Num:
        if min > n:
            min = n
    return min


min = MinElem(Num)
print("There is min element: {0}".format(min))

def MaxElem(Num):
    max = Num[0]
    for n in Num:
        if max < n:
            max = n
    return max


max = MaxElem(Num)
print("There is min element: {0}".format(max))